ps -aux | grep manager | grep -v grep
grep '7[1|3|5]' nums.txt
grep '\bs\w*r\b' words.txt
grep -E -o "\b[A-Za-z0-9._]+@[A-Za-z0-9.-]+\.[A-Za-z]{2,5}\b" mails.txt
grep -E -o "(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\.(25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)" IP.txt
grep -E -o "([0-3][0-9][0-9])" Tnum.txt
